@model ProjetoEngSoftII.Models.Vacinas.Vacinador

@{
    ViewData["Title"] = "Deletar Vacinador";
}

<h1>Deletar Vacinador</h1>

<h3>Você tem certeza que deseja deletar o vacinador?</h3>
<div>
    <h4>Vacinador</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.RegistroProfissional)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.RegistroProfissional)
        </dd>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>

    <form asp-action="DeletarVacinador">
        <input type="hidden" asp-for="RegistroProfissional" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> |
        <a asp-action="IndexVacinadores">Voltar</a>
    </form>
</div>
